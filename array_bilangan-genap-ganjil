#include <iostream>
using namespace std;

// program untuk mementukan bilangan bulat pada array
int main() {
    int numbers[] = {2, 5, 8, 10, 15};
    //int evenCount = 0;

    for(int i = 0; i < 5; i++) {
        if(numbers[i] % 2 == 0) { //untuk menentukan bilangan ganjil ubah jadi != (tidak sama dengan)
            cout << numbers[i] << endl; 
        }
    }
    return 0;
}
